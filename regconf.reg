Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\Directory\Background\shell\folderFlattener]
@="Flatten Folder"
"Icon"="powershell.exe"

[HKEY_CLASSES_ROOT\Directory\Background\shell\folderFlattener\command]
@="powershell.exe -NoExit -ExecutionPolicy Bypass -File \"%APPDATA%/folderFlattener\" -WorkingDir \"%V\""
